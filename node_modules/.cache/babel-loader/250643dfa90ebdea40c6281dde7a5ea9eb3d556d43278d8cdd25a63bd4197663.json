{"ast":null,"code":"export default {\n  data() {\n    return {\n      showModal: true,\n      username: '',\n      email: '',\n      phone: '',\n      password: '',\n      confirmPassword: '',\n      passwordFieldType: 'password',\n      passwordIcon: '/path/to/eye-icon.png' // Replace with path to your eye icon\n    };\n  },\n\n  methods: {\n    closeModal() {\n      this.showModal = false;\n    },\n    register() {\n      // Handle registration logic here\n    },\n    togglePasswordVisibility() {\n      this.passwordFieldType = this.passwordFieldType === 'password' ? 'text' : 'password';\n      // Replace with path to your eye slash icon when password is visible\n      this.passwordIcon = this.passwordFieldType === 'password' ? '/path/to/eye-icon.png' : '/path/to/eye-slash-icon.png';\n    }\n  }\n};","map":{"version":3,"names":["data","showModal","username","email","phone","password","confirmPassword","passwordFieldType","passwordIcon","methods","closeModal","register","togglePasswordVisibility"],"sources":["/Users/annasuslova/cpp_lab07/src/components/Authorization/ModalRegister.vue"],"sourcesContent":["<template>\r\n  <div class=\"modal\" v-if=\"showModal\">\r\n    <div class=\"modal-background\" @click=\"closeModal\"></div>\r\n    <div class=\"modal-content\">\r\n      <div class=\"modal-header\">\r\n        <h3>Registration form</h3>\r\n        <button class=\"close-button\" @click=\"closeModal\">X</button>\r\n      </div>\r\n      <div class=\"modal-body\">\r\n        <form @submit.prevent=\"register\">\r\n          <div class=\"field\">\r\n            <label for=\"username\">User name</label>\r\n            <input type=\"text\" id=\"username\" v-model=\"username\" required>\r\n          </div>\r\n          \r\n          <div class=\"field\">\r\n            <label for=\"email\">Email</label>\r\n            <input type=\"email\" id=\"email\" v-model=\"email\" placeholder=\"example@mail.com\" required>\r\n          </div>\r\n          \r\n          <div class=\"field\">\r\n            <label for=\"phone\">Phone number</label>\r\n            <input type=\"tel\" id=\"phone\" v-model=\"phone\" placeholder=\"+1234567890\" required>\r\n          </div>\r\n          \r\n          <div class=\"field\">\r\n            <label for=\"password\">Password</label>\r\n            <div class=\"password-container\">\r\n              <input :type=\"passwordFieldType\" id=\"password\" v-model=\"password\" required>\r\n              <button type=\"button\" class=\"toggle-password\" @mousedown=\"togglePasswordVisibility\" @mouseup=\"togglePasswordVisibility\" @mouseleave=\"togglePasswordVisibility\">\r\n                <img :src=\"passwordIcon\" alt=\"Toggle Password Visibility\">\r\n              </button>\r\n            </div>\r\n          </div>\r\n          \r\n          <div class=\"field\">\r\n            <label for=\"confirmPassword\">Confirm your password</label>\r\n            <div class=\"password-container\">\r\n              <input :type=\"passwordFieldType\" id=\"confirmPassword\" v-model=\"confirmPassword\" required>\r\n              <button type=\"button\" class=\"toggle-password\" @mousedown=\"togglePasswordVisibility\" @mouseup=\"togglePasswordVisibility\" @mouseleave=\"togglePasswordVisibility\">\r\n                <img :src=\"passwordIcon\" alt=\"Toggle Password Visibility\">\r\n              </button>\r\n            </div>\r\n          </div>\r\n\r\n          <button type=\"submit\" class=\"signup-button\">Sign up</button>\r\n        </form>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      showModal: true,\r\n      username: '',\r\n      email: '',\r\n      phone: '',\r\n      password: '',\r\n      confirmPassword: '',\r\n      passwordFieldType: 'password',\r\n      passwordIcon: '/path/to/eye-icon.png' // Replace with path to your eye icon\r\n    };\r\n  },\r\n  methods: {\r\n    closeModal() {\r\n      this.showModal = false;\r\n    },\r\n    register() {\r\n      // Handle registration logic here\r\n    },\r\n    togglePasswordVisibility() {\r\n      this.passwordFieldType = this.passwordFieldType === 'password' ? 'text' : 'password';\r\n      // Replace with path to your eye slash icon when password is visible\r\n      this.passwordIcon = this.passwordFieldType === 'password' ? '/path/to/eye-icon.png' : '/path/to/eye-slash-icon.png';\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n/* Your existing modal styles ... */\r\n\r\n.field {\r\n  margin-bottom: 10px;\r\n}\r\n\r\n.field label {\r\n  display: block;\r\n  margin-bottom: 5px;\r\n}\r\n\r\n.field input[type=\"text\"],\r\n.field input[type=\"email\"],\r\n.field input[type=\"tel\"],\r\n.field input[type=\"password\"] {\r\n  width: 100%;\r\n  padding: 8px;\r\n  border: 1px solid #ccc;\r\n  border-radius: 4px;\r\n}\r\n\r\n.password-container {\r\n  display: flex;\r\n}\r\n\r\n.toggle-password {\r\n  background: none;\r\n  border: none;\r\n  cursor: pointer;\r\n}\r\n\r\n.signup-button {\r\n  background-color: #4CAF50;\r\n  color: white;\r\n  padding: 10px 20px;\r\n  border: none;\r\n  border-radius: 4px;\r\n  cursor: pointer;\r\n}\r\n\r\n.signup-button:hover {\r\n  background-color: #45a049;\r\n}\r\n</style>\r\n"],"mappings":"AAqDA,eAAe;EACbA,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,SAAS,EAAE,IAAI;MACfC,QAAQ,EAAE,EAAE;MACZC,KAAK,EAAE,EAAE;MACTC,KAAK,EAAE,EAAE;MACTC,QAAQ,EAAE,EAAE;MACZC,eAAe,EAAE,EAAE;MACnBC,iBAAiB,EAAE,UAAU;MAC7BC,YAAY,EAAE,uBAAsB,CAAE;IACxC,CAAC;EACH,CAAC;;EACDC,OAAO,EAAE;IACPC,UAAUA,CAAA,EAAG;MACX,IAAI,CAACT,SAAQ,GAAI,KAAK;IACxB,CAAC;IACDU,QAAQA,CAAA,EAAG;MACT;IAAA,CACD;IACDC,wBAAwBA,CAAA,EAAG;MACzB,IAAI,CAACL,iBAAgB,GAAI,IAAI,CAACA,iBAAgB,KAAM,UAAS,GAAI,MAAK,GAAI,UAAU;MACpF;MACA,IAAI,CAACC,YAAW,GAAI,IAAI,CAACD,iBAAgB,KAAM,UAAS,GAAI,uBAAsB,GAAI,6BAA6B;IACrH;EACF;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}