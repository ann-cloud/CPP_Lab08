{"ast":null,"code":"import BuildingsList from './BuildingsList.vue';\nimport AddBuildingModal from './AddBuildingModal.vue';\nimport axios from 'axios';\nexport default {\n  components: {\n    BuildingsList,\n    AddBuildingModal\n  },\n  name: 'ProfileContentComponent',\n  data() {\n    return {\n      username: this.$store.getters.getUsername,\n      numberOfBuildings: '',\n      showModal: false\n    };\n  },\n  methods: {\n    async getNumberOfBuildingsByUserId() {\n      try {\n        const userId = this.$store.getters.getUserId;\n        const response = await axios.get('http://localhost:8080/api/buildings');\n        console.log;\n        const userBuildings = response.data.filter(building => building.user_id === userId);\n        this.numberOfBuildings = userBuildings.length;\n      } catch (error) {\n        alert('Building count fetch failed!');\n        console.error('Error fetching buildings:', error.response.data.message);\n      }\n    }\n  },\n  created() {\n    this.getNumberOfBuildingsByUserId();\n  }\n};","map":{"version":3,"names":["BuildingsList","AddBuildingModal","axios","components","name","data","username","$store","getters","getUsername","numberOfBuildings","showModal","methods","getNumberOfBuildingsByUserId","userId","getUserId","response","get","console","log","userBuildings","filter","building","user_id","length","error","alert","message","created"],"sources":["/Users/annasuslova/cpp_lab07/src/components/Profile/ProfileContent.vue"],"sourcesContent":["<template>\r\n  <div class=\"profile-content\">\r\n    <div class=\"add-building-btn\">\r\n      <button @click=\"showModal = true\">Add building</button>\r\n    </div>\r\n    <div class=\"user-content\">\r\n      <div class=\"user-info\">\r\n        <h2>Information about user</h2>\r\n        <div class=\"info-group\">\r\n            <p><strong>Email:</strong></p> \r\n            <p>{{username}}</p> \r\n        </div>\r\n        <div class=\"info-group\">\r\n            <p><strong>Number of buildings:</strong></p> \r\n            <p>5</p>\r\n        </div>\r\n      </div>\r\n      <BuildingsList />\r\n      <AddBuildingModal v-if=\"showModal\" @closeAddModal=\"showModal = false\"/>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport BuildingsList from './BuildingsList.vue';\r\nimport AddBuildingModal from './AddBuildingModal.vue';\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  components: {\r\n    BuildingsList,\r\n    AddBuildingModal\r\n  },\r\n  name: 'ProfileContentComponent',\r\n  data() {\r\n    return {\r\n      username: this.$store.getters.getUsername,\r\n      numberOfBuildings: '',\r\n      showModal: false,\r\n    }\r\n  },\r\n  methods: {\r\n    async getNumberOfBuildingsByUserId() {\r\n      try {    \r\n        const userId = this.$store.getters.getUserId;\r\n        const response = await axios.get('http://localhost:8080/api/buildings');\r\n\r\n        console.log\r\n\r\n        const userBuildings = response.data.filter(building => building.user_id === userId);\r\n\r\n        this.numberOfBuildings = userBuildings.length;\r\n\r\n      } catch (error) {\r\n        alert('Building count fetch failed!');\r\n        console.error('Error fetching buildings:', error.response.data.message);\r\n      }\r\n    }\r\n  },\r\n  created() {\r\n    this.getNumberOfBuildingsByUserId();\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n\r\n.user-content\r\n{\r\n  display: flex;\r\n  justify-content: space-between;\r\n}\r\n.add-building-btn\r\n{\r\n  display: flex;\r\n  justify-content: flex-end;\r\n  padding: 40px 60px;\r\n}\r\n\r\n.add-building-btn button\r\n{\r\n  background-color: #417B5A;\r\n  border: 0.5px solid rgba(0, 0, 0, 0.5);\r\n  border-radius: 5px;\r\n  box-shadow: 0 2px 5px rgba(0, 0, 0, 0.2);\r\n  padding: 20px 30px;\r\n  color: white;\r\n  font-size: 16px;\r\n  font-weight: bold;\r\n}\r\n\r\n.add-building-btn button:hover {\r\n  background-color: #45a049;\r\n  box-shadow: 0 5px 15px rgba(0, 0, 0, 0.3); /* Shadow on hover */\r\n}\r\n.info-group\r\n{\r\n  display: flex;\r\n  flex-wrap: nowrap;\r\n  flex-direction: row;\r\n  font-size: large;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n  color: white;\r\n}\r\n\r\n.user-info {\r\n  background-color: #9197AE;\r\n  border-radius: 5px;\r\n  padding: 20px 40px;\r\n  width: 100%;\r\n  max-width: 300px; \r\n  margin: 0px 60px; \r\n  box-shadow: 0 5px 15px rgba(0, 0, 0, 0.3);\r\n}\r\n\r\n.user-info h2 {\r\n  text-align: center;\r\n  color: #ffffff;\r\n  margin-bottom: 15px;\r\n}\r\n\r\n</style>\r\n"],"mappings":"AAwBA,OAAOA,aAAY,MAAO,qBAAqB;AAC/C,OAAOC,gBAAe,MAAO,wBAAwB;AACrD,OAAOC,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,UAAU,EAAE;IACVH,aAAa;IACbC;EACF,CAAC;EACDG,IAAI,EAAE,yBAAyB;EAC/BC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,IAAI,CAACC,MAAM,CAACC,OAAO,CAACC,WAAW;MACzCC,iBAAiB,EAAE,EAAE;MACrBC,SAAS,EAAE;IACb;EACF,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,4BAA4BA,CAAA,EAAG;MACnC,IAAI;QACF,MAAMC,MAAK,GAAI,IAAI,CAACP,MAAM,CAACC,OAAO,CAACO,SAAS;QAC5C,MAAMC,QAAO,GAAI,MAAMd,KAAK,CAACe,GAAG,CAAC,qCAAqC,CAAC;QAEvEC,OAAO,CAACC,GAAE;QAEV,MAAMC,aAAY,GAAIJ,QAAQ,CAACX,IAAI,CAACgB,MAAM,CAACC,QAAO,IAAKA,QAAQ,CAACC,OAAM,KAAMT,MAAM,CAAC;QAEnF,IAAI,CAACJ,iBAAgB,GAAIU,aAAa,CAACI,MAAM;MAE/C,EAAE,OAAOC,KAAK,EAAE;QACdC,KAAK,CAAC,8BAA8B,CAAC;QACrCR,OAAO,CAACO,KAAK,CAAC,2BAA2B,EAAEA,KAAK,CAACT,QAAQ,CAACX,IAAI,CAACsB,OAAO,CAAC;MACzE;IACF;EACF,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACf,4BAA4B,CAAC,CAAC;EACrC;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}