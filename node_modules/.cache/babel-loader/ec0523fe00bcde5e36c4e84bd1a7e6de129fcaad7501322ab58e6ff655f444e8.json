rue\">Add building</button>\r\n    </div>\r\n    <div class=\"user-content\">\r\n      <div class=\"user-info\">\r\n        <h2>Information about user</h2>\r\n        <div class=\"info-group\">\r\n            <p><strong>Email:</strong></p> \r\n            <p>{{username}}</p> \r\n        </div>\r\n        <div class=\"info-group\">\r\n            <p><strong>Number of buildings:</strong></p> \r\n            <p>5</p>\r\n        </div>\r\n      </div>\r\n      <BuildingsList />\r\n      <AddBuildingModal v-if=\"showModal\" @closeAddModal=\"showModal = false\"/>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport BuildingsList from './BuildingsList.vue';\r\nimport AddBuildingModal from './AddBuildingModal.vue';\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  components: {\r\n    BuildingsList,\r\n    AddBuildingModal\r\n  },\r\n  name: 'ProfileContentComponent',\r\n  data() {\r\n    return {\r\n      username: this.$store.getters.getUsername,\r\n      numberOfBuildings: '',\r\n      showModal: false,\r\n    }\r\n  },\r\n  methods: {\r\n    closeModal() {\r\n      this.showModal = false;\r\n      this.$emit('logoutConfirmed');\r\n    },\r\n    async confirmLogout() {\r\n      try {    \r\n        const userId = this.$store.getters.getUserId;\r\n        const response = await axios.delete(`http://localhost:8080/api/auth/refreshtoken/${userId}`);\r\n        axios.defaults.headers.common['Authorization'] = \"\";\r\n\r\n        localStorage.removeItem('accessToken');\r\n        this.$store.dispatch('authenticate', {\r\n            authenticated: false,\r\n            userId: \"\",\r\n            username: \"\",\r\n            accessToken: \"\",\r\n          });\r\n\r\n        console.log('Logout successful:', response.data);\r\n        this.$router.push('/sign-in');\r\n        this.closeModal();\r\n      } catch (error) {\r\n        alert('Logout failed!');\r\n        console.error('Error logging out:', error.response.data.message);\r\n      }\r\n   