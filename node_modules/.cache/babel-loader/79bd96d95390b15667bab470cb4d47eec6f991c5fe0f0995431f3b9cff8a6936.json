{"ast":null,"code":"import L from 'leaflet';\nimport 'leaflet/dist/leaflet.css';\nimport axios from 'axios';\nimport markerIcon from '@/assets/img/location.png';\nimport SimulationInfo from './SimulationInfo.vue';\nimport AlertPopup from './AlertPopup.vue';\nexport default {\n  name: \"SimulationContent\",\n  components: {\n    SimulationInfo,\n    AlertPopup\n  },\n  data() {\n    return {\n      map: null,\n      operation: \"Pause\",\n      buildings: []\n    };\n  },\n  computed: {\n    calculateCenter() {\n      if (this.buildings.length === 0) {\n        // Default center if there are no markers\n        return [0, 0];\n      }\n\n      // Calculate the average coordinates\n      const sumX = this.buildings.reduce((sum, building) => sum + building.coordinateX, 0);\n      const sumY = this.buildings.reduce((sum, building) => sum + building.coordinateY, 0);\n      const averageX = sumX / this.buildings.length;\n      const averageY = sumY / this.buildings.length;\n      return [averageX, averageY];\n    }\n  },\n  methods: {\n    async getBuildingsOfUser() {\n      try {\n        const userId = this.$store.getters.getUserId;\n        if (userId !== undefined && !isNaN(userId) && userId != null) {\n          const promise = axios.get('http://localhost:8080/api/data/buildings/getBuildingsByUserId/' + userId, {\n            headers: {\n              \"Authorization\": `Bearer ${this.$store.getters.getAccessToken}`\n            }\n          });\n          promise.then(response => {\n            this.buildings = response.data;\n            this.updateMap();\n          });\n        }\n      } catch (error) {\n        alert('Building fetch failed!');\n        console.error('Error fetching buildings:', error.response.data.message);\n      }\n    },\n    handlePause() {\n      if (this.operation == \"Pause\") {\n        this.pauseSimulation();\n      } else {\n        this.resumeSimulation();\n      }\n    },\n    async startSimulation() {\n      try {\n        const userId = this.$store.getters.getUserId;\n        if (userId !== undefined && !isNaN(userId) && userId != null) {\n          const promise = axios.post('http://localhost:8080/api/data/simulation', {\n            headers: {\n              \"Authorization\": `Bearer ${this.$store.getters.getAccessToken}`\n            }\n          });\n          promise.then(response => {\n            alert(response.data);\n          });\n        }\n      } catch (error) {\n        alert('Simulation start failed!');\n        console.error('Error starting simulation:', error.response.data.message);\n      }\n    },\n    async pauseSimulation() {\n      try {\n        const userId = this.$store.getters.getUserId;\n        if (userId !== undefined && !isNaN(userId) && userId != null) {\n          const promise = axios.patch('http://localhost:8080/api/data/simulation/pause', {\n            headers: {\n              \"Authorization\": `Bearer ${this.$store.getters.getAccessToken}`\n            }\n          });\n          promise.then(response => {\n            alert(response.data);\n            this.operation = \"Resume\";\n          });\n        }\n      } catch (error) {\n        alert('Simulation pause failed!');\n        console.error('Error pausing simulation:', error.response.data.message);\n      }\n    },\n    async resumeSimulation() {\n      try {\n        const userId = this.$store.getters.getUserId;\n        if (userId !== undefined && !isNaN(userId) && userId != null) {\n          const promise = axios.patch('http://localhost:8080/api/data/simulation/resume', {\n            headers: {\n              \"Authorization\": `Bearer ${this.$store.getters.getAccessToken}`\n            }\n          });\n          promise.then(response => {\n            alert(response.data);\n            this.operation = \"Pause\";\n          });\n        }\n      } catch (error) {\n        alert('Simulation resume failed!');\n        console.error('Error resuming simulation:', error.response.data.message);\n      }\n    },\n    async stopSimulation() {\n      try {\n        const userId = this.$store.getters.getUserId;\n        if (userId !== undefined && !isNaN(userId) && userId != null) {\n          const promise = axios.patch('http://localhost:8080/api/data/simulation/stop', {\n            headers: {\n              \"Authorization\": `Bearer ${this.$store.getters.getAccessToken}`\n            }\n          });\n          promise.then(response => {\n            alert(response.data);\n          });\n        }\n      } catch (error) {\n        alert('Simulation stop failed!');\n        console.error('Error stopping simulation:', error.response.data.message);\n      }\n    },\n    updateMap() {\n      this.map.eachLayer(layer => {\n        if (layer instanceof L.Marker) {\n          this.map.removeLayer(layer);\n        }\n      });\n      const locationIcon = L.icon({\n        iconUrl: markerIcon,\n        iconSize: [32, 32],\n        iconAnchor: [16, 32],\n        popupAnchor: [0, -32]\n      });\n      console.log(this.buildings);\n      this.buildings.forEach(building => {\n        L.marker(this.calculateCenter, {\n          icon: locationIcon\n        }).addTo(this.map).bindPopup(`<b>Building ${building.id}</b><br>${building.address}`);\n      });\n    }\n  },\n  mounted() {\n    this.map = L.map('map').setView([0, 0], 2);\n    L.tileLayer('https://{s}.tile.openstreetmap.org/{z}/{x}/{y}.png', {\n      attribution: 'Â© OpenStreetMap contributors'\n    }).addTo(this.map);\n    this.getBuildingsOfUser();\n  }\n};","map":{"version":3,"names":["L","axios","markerIcon","SimulationInfo","AlertPopup","name","components","data","map","operation","buildings","computed","calculateCenter","length","sumX","reduce","sum","building","coordinateX","sumY","coordinateY","averageX","averageY","methods","getBuildingsOfUser","userId","$store","getters","getUserId","undefined","isNaN","promise","get","headers","getAccessToken","then","response","updateMap","error","alert","console","mes