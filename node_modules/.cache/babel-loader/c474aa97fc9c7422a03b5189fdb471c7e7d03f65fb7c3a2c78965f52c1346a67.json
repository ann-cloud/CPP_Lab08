{"ast":null,"code":"import ModalConfirmLogout from '@/components/Authorization/ModalConfirmLogout.vue';\nexport default {\n  components: {\n    ModalConfirmLogout\n  },\n  name: 'HeaderUserOverlayComponent',\n  data() {\n    return {\n      showModal: false,\n      currentRoute: this.$route.path // Initial value based on the current route\n    };\n  },\n\n  computed: {\n    authenticated() {\n      return this.$store.getters.isAuthenticated;\n    }\n  },\n  watch: {\n    '$route'(to) {\n      // Update currentRoute when the route changes\n      this.currentRoute = to.path;\n    }\n  },\n  methods: {\n    handleButtonClick() {\n      this.showModal = true;\n    },\n    closeOverlay() {\n      this.$emit('close-overlay');\n    }\n  }\n};","map":{"version":3,"names":["ModalConfirmLogout","components","name","data","showModal","currentRoute","$route","path","computed","authenticated","$store","getters","isAuthenticated","watch","to","methods","handleButtonClick","closeOverlay","$emit"],"sources":["/Users/annasuslova/cpp_lab07/src/components/HeaderUserOverlay.vue"],"sourcesContent":["<template>\n  <div class=\"header-user-overlay\" @mouseleave=\"closeOverlay\">\n    <router-link to=\"/sign-in\">\n      <div class=\"overlay-row\" v-if=\"!authenticated && currentRoute !== '/sign-in'\">Log in</div>\n    </router-link>\n    <router-link to=\"/sign-up\">\n      <div class=\"overlay-row\" v-if=\"!authenticated && currentRoute !== '/sign-up'\">Sign up</div>\n    </router-link>\n    <router-link to=\"/profile\">\n      <div class=\"overlay-row\" v-if=\"authenticated && currentRoute !== '/profile'\">Profile</div>\n    </router-link>\n    <router-link to=\"/simulation\">\n      <div class=\"overlay-row\" v-if=\"authenticated && currentRoute !== '/simulation'\">Simulation</div>\n    </router-link>\n    <div class=\"overlay-row\" v-if=\"authenticated\" id=\"logout\" @click=\"handleButtonClick\">Log out</div>\n    <ModalConfirmLogout v-if=\"showModal\" @logoutConfirmed=\"closeOverlay\"/>\n  </div>\n</template>\n\n<script>\nimport ModalConfirmLogout from '@/components/Authorization/ModalConfirmLogout.vue';\n\nexport default {\n  components: {\n    ModalConfirmLogout,\n  },\n  name: 'HeaderUserOverlayComponent',\n  data() {\n    return {\n      showModal: false,\n      currentRoute: this.$route.path, // Initial value based on the current route\n    };\n  },\n  computed: {\n    authenticated() {\n      return this.$store.getters.isAuthenticated;\n    },\n  },\n  watch: {\n    '$route'(to) {\n      // Update currentRoute when the route changes\n      this.currentRoute = to.path;\n    },\n  },\n  methods: {\n    handleButtonClick() {\n      this.showModal = true;\n    },\n    closeOverlay() {\n      this.$emit('close-overlay');\n    },\n  },\n};\n</script>\n\n<style scoped>\n.header-user-overlay {\n  position: absolute;\n  top: 45px;\n  right: 0;\n  background-color: #848AA0;\n  color: white;\n  padding: 10px;\n  display: flex;\n  flex-direction: column;\n  min-width: 120px;\n  border-radius: 5px;\n}\n\n.overlay-row {\n  padding: 5px;\n  cursor: pointer;\n  border-radius: 5px;\n}\n\n.overlay-row:hover {\n  background-color: #2c3e50;\n}\n</style>\n  "],"mappings":"AAoBA,OAAOA,kBAAiB,MAAO,mDAAmD;AAElF,eAAe;EACbC,UAAU,EAAE;IACVD;EACF,CAAC;EACDE,IAAI,EAAE,4BAA4B;EAClCC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,SAAS,EAAE,KAAK;MAChBC,YAAY,EAAE,IAAI,CAACC,MAAM,CAACC,IAAI,CAAE;IAClC,CAAC;EACH,CAAC;;EACDC,QAAQ,EAAE;IACRC,aAAaA,CAAA,EAAG;MACd,OAAO,IAAI,CAACC,MAAM,CAACC,OAAO,CAACC,eAAe;IAC5C;EACF,CAAC;EACDC,KAAK,EAAE;IACL,QAAQP,CAACQ,EAAE,EAAE;MACX;MACA,IAAI,CAACT,YAAW,GAAIS,EAAE,CAACP,IAAI;IAC7B;EACF,CAAC;EACDQ,OAAO,EAAE;IACPC,iBAAiBA,CAAA,EAAG;MAClB,IAAI,CAACZ,SAAQ,GAAI,IAAI;IACvB,CAAC;IACDa,YAAYA,CAAA,EAAG;MACb,IAAI,CAACC,KAAK,CAAC,eAAe,CAAC;IAC7B;EACF;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}